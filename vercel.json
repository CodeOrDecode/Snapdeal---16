{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        },
        {
            "source": "/men(.*)",
            "destination": "/men"
        },
        {
            "source": "/women(.*)",
            "destination": "/women"
        },
        {
            "source": "/product(.*)",
            "destination": "/product"
        },
        {
            "source": "/afterlogin(.*)",
            "destination": "/afterlogin"
        }
    ]
}